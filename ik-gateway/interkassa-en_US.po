msgid "Выбрать платежную систему"
msgstr "Select payment system"

msgid "Выберите удобный способ оплаты"
msgstr "Select a convenient payment method"

msgid "Укажите валюту"
msgstr "Specify currency"

msgid "Нажмите &laquo;Оплатить&raquo;"
msgstr "Click &laquo;Pay&raquo;"

msgid "Оплатить через"
msgstr "Pay by"

msgid "Вы не выбрали валюту"
msgstr "You have not selected a currency"

msgid "Интеркасса 2.0"
msgstr "Interkassa 2.0"

msgid "Интеркасса"
msgstr "Interkassa"

msgid "Шлюз отключен"
msgstr "Gateway is disabled"

msgid "Вкл. / Выкл."
msgstr "On / Of"

msgid "Включить"
msgstr "On"

msgid "Тестовый режим"
msgstr "Test mode"

msgid "Включить тестовый режим"
msgstr "Enable test mode"

msgid "Тестовый ключ"
msgstr "Test key"

msgid "Введите тестовый ключ"
msgstr "Enter test key"

msgid "Заголовок"
msgstr "Header"

msgid "Заголовок, который отображается на странице оформления заказа"
msgstr "The title that appears on the ordering page"

msgid "Описание"
msgstr "Description"

msgid "Описание, которое отображается в процессе выбора формы оплаты"
msgstr "The description that is displayed during the payment method selection process"

msgid "Оплатить"
msgstr "Pay"

msgid "Оплатить через электронную платежную систему Интеркасса"
msgstr "Pay through the electronic payment system of the Interkassa"

msgid "Индефикатор кассы"
msgstr "Cashier ID"

msgid "Уникальный идентификатор кассы в системе Интеркасса."
msgstr "Unique cash register identifier in the Interkassa system."

msgid "Секретный ключ"
msgstr "The secret key"

msgid "Включить API/Отключить API"
msgstr "API On/Of"

msgid "Включить API"
msgstr "API ON"

msgid "Находится в настройках аккаунта в разделе API."
msgstr "Located in the account settings in the API section."

msgid "Спасибо за Ваш заказ, пожалуйста, нажмите кнопку ниже, чтобы заплатить."
msgstr "Thank you for your order, please click the button below to pay."

msgid "Платеж успешно оплачен через Интеркассу"
msgstr "The payment was successfully paid through the Interkassa"

msgid "Платеж не оплачен"
msgstr "Payment not paid"

msgid "Отказаться от оплаты & вернуться в корзину"
msgstr "Refuse to pay & return to shopping cart"
